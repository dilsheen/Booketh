<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.bellatrix.booketh">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WriterProfileActivity" />
        <activity android:name=".AvailableToFundActivity" />
        <activity android:name=".AddBookActivity" />
        <activity android:name=".CreateBookActivity" />
        <activity android:name=".BookDetailsActivity" />
        <activity android:name=".FundProjectActivity" />
        <activity android:name=".TrendingBooksActivity" />
        <activity android:name=".FundFormActivity" />
        <activity android:name=".ConfirmationActivity"/>
        <activity android:name=".AdvertiseActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".TransactionHistoryActivity"></activity>
    </application>

</manifest>